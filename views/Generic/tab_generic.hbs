<div class="inputbox">
  <table id="example" class="display" style="width:100%">
  <thead>
    <tr> 
         {{#each tab_columns}}
            <th>{{@key}}</th>
         {{/each}}
    </tr>
  </thead>
  <tbody>
  {{#each data}}
    <tr>
      {{#each this}}
        <td>{{.}}</td>  <!-- permet de récupérer la valeur du champ de l'objet JSON -->
      {{/each}}
    </tr>
  {{/each}}
  </tbody>  
  </table>
</div>
<script>
  document.addEventListener('DOMContentLoaded', function () {
    let table = new DataTable('#example');
});
</script>